{
  "name": "deploy-mdbook",
  "version": "1.0.0",
  "private": true,
  "description": "Automatically build and deploy mdbook",
  "main": "lib/main.js",
  "scripts": {
    "watch": "tsc -w",
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "XAMPPRocky",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.3.0",
    "@actions/exec": "=1.1.0",
    "@actions/github": "^5.0.0",
    "@jamesives/github-pages-deploy-action": "^4.1.4",
    "get-github-release": "^1.0.4"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.9",
    "@typescript-eslint/parser": "^4.8.1",
    "@vercel/ncc": "^0.25.1",
    "eslint": "^7.17.0",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-jest": "^24.1.3",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "js-yaml": "^3.14.0",
    "prettier": "2.2.1",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  }
}
