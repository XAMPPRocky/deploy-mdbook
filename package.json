{
  "name": "deploy-mdbook",
  "version": "1.0.0",
  "private": true,
  "description": "Automatically build and deploy mdbook",
  "main": "lib/main.js",
  "scripts": {
    "watch": "tsc -w",
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "XAMPPRocky",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.2",
    "@actions/exec": "=1.0.2",
    "get-github-release": "^1.0.4",
    "github-pages-deploy-action": "^3.4.5"
  },
  "devDependencies": {
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.29",
    "@typescript-eslint/parser": "^2.21.0",
    "@zeit/ncc": "^0.20.5",
    "eslint": "^5.16.0",
    "eslint-plugin-github": "^2.0.0",
    "eslint-plugin-jest": "^22.21.0",
    "jest": "^24.9.0",
    "jest-circus": "^24.9.0",
    "js-yaml": "^3.13.1",
    "prettier": "^1.19.1",
    "ts-jest": "^24.3.0",
    "typescript": "^3.8.3"
  }
}
